{
    "name": "risetechapps/monitoring-for-laravel",
    "description": "",
    "keywords": [
        "risetech",
        "logs",
        "monitoring",
        "laravel"
    ],
    "homepage": "https://github.com/risetechapps/monitoring-for-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "RiseTechApps",
            "email": "apps@risetech.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "aws/aws-sdk-php": "^3.363",
        "hisorange/browser-detect": "^4.5",
        "risetechapps/risetools": "^1.3.0",
        "laravel/sanctum": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "RiseTechApps\\Monitoring\\": "src"
        },
        "files": [
            "/src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RiseTechApps\\Monitoring\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "extra": {
        "laravel": {
            "providers": [
                "RiseTechApps\\Monitoring\\MonitoringServiceProvider"
            ],
            "aliases": {
                "Logs": "RiseTechApps\\Monitoring\\MonitoringFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "version": "1.2.0"
}
